{
  "version": "2.8",
  "manifest_version": 2,
  "minimum_chrome_version" : "36",
  "name": "Lucubrate",
  "author": "Joshua Froelich",
  "homepage_url": "https://github.com/jfroelich/rss-reader",
  "description": "Reading by artificial light late into the night",
  "browser_action": {
    "default_icon": "/media/rss_icon_trans.png",
    "default_title": "Lucubrate"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/lib/async.js",
      "/lib/URI.js",
      "/src/archive.js",
      "/src/augment.js",
      "/src/badge.js",
      "/src/background.js",
      "/src/database.js",
      "/src/images.js",
      "/src/install.js",
      "/src/notifications.js",
      "/src/rewrite.js",
      "/src/feeds.js",
      "/src/polling.js",
      "/src/resolve-elements.js",
      "/src/deserialize-feed.js",
      "/src/string-utilities.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "background",
    "idle",
    "notifications"
  ],
  "content_security_policy":
    "script-src 'self'; object-src 'self' https://vimeo.com https://www.youtube.com https://www.ustream.tv; frame-src http: https:"
}
