{
  "version": "110",
  "manifest_version": 2,
  "minimum_chrome_version" : "47",
  "name": "Lucubrate",
  "author": "Joshua Froelich",
  "homepage_url": "https://github.com/jfroelich/rss-reader",
  "description": "Reading by artificial light late into the night",
  "browser_action": {
    "default_icon": "/media/rss_icon_trans.png",
    "default_title": "Lucubrate"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/src/archive-entries.js",
      "/src/async.js",
      "/src/augment-entry-content.js",
      "/src/background.js",
      "/src/database.js",
      "/src/deserialize-feed.js",
      "/src/entry-store.js",
      "/src/feed-poll.js",
      "/src/feed-store.js",
      "/src/fetch-feed.js",
      "/src/fetch-html.js",
      "/src/fetch-image-dimensions.js",
      "/src/get-schemeless-url.js",
      "/src/html-document-wrapper.js",
      "/src/is-valid-date.js",
      "/src/resolve-document-urls.js",
      "/src/rewrite-url.js",
      "/src/show-notification.js",
      "/src/string-utils.js",
      "/src/update-badge.js",
      "/src/URI.js",
      "/src/utf8.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "background",
    "idle",
    "notifications"
  ],
  "content_security_policy":
    "script-src 'self'; object-src 'self' https://vimeo.com https://www.youtube.com https://www.ustream.tv; frame-src http: https:"
}
