{
  "version": "110",
  "manifest_version": 2,
  "minimum_chrome_version" : "47",
  "name": "Lucubrate",
  "author": "Joshua Froelich",
  "homepage_url": "https://github.com/jfroelich/rss-reader",
  "description": "Reading by artificial light late into the night",
  "browser_action": {
    "default_icon": "/images/rss_icon_trans.png",
    "default_title": "Lucubrate"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/lib/parse-srcset.js",
      "/src/add-entry.js",
      "/src/archive.js",
      "/src/background.js",
      "/src/badge.js",
      "/src/dom-filters/resolve-document-urls.js",
      "/src/dom-filters/set-image-dimensions.js",
      "/src/dom-filters/filter-sourceless-images.js",
      "/src/dom-filters/filter-tracking-images.js",
      "/src/dom-filters/transform-lazy-images.js",
      "/src/entry.js",
      "/src/favicon.js",
      "/src/feed.js",
      "/src/feed-parser.js",
      "/src/fetch-feed.js",
      "/src/fetch-html.js",
      "/src/indexeddb.js",
      "/src/notify.js",
      "/src/poll.js",
      "/src/rewrite-url.js",
      "/src/string-utils.js",
      "/src/update-feed.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "http://*/*",
    "https://*/*",
    "idle",
    "notifications",
    "tabs",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "background",
    "idle",
    "notifications"
  ],
  "content_security_policy":
    "script-src 'self'; object-src 'self' https://vimeo.com https://www.youtube.com https://www.ustream.tv; frame-src http: https:"
}
