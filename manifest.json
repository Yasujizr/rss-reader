{
  "version": "110",
  "manifest_version": 2,
  "minimum_chrome_version" : "47",
  "name": "Lucubrate",
  "author": "Joshua Froelich",
  "homepage_url": "https://github.com/jfroelich/rss-reader",
  "description": "Reading by artificial light late into the night",
  "browser_action": {
    "default_icon": "/media/rss_icon_trans.png",
    "default_title": "Lucubrate"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/lib/async.js",
      "/lib/parse-srcset.js",
      "/lib/URI.js",
      "/lib/utf8.js",
      "/src/archive/archive-entries.js",
      "/src/document-transform/fetch-image-dimensions.js",
      "/src/document-transform/resolve-document-urls.js",
      "/src/document-transform/transform-lazy-images.js",
      "/src/dom/html-document-wrapper.js",
      "/src/extension/on-browser-action-clicked.js",
      "/src/extension/on-runtime-installed.js",
      "/src/extension/show-notification.js",
      "/src/extension/update-badge.js",
      "/src/fetch/fetch-html.js",
      "/src/fetch/fetch-image.js",
      "/src/html/replace-html.js",
      "/src/html/parse-html.js",
      "/src/lang/filter-control-characters.js",
      "/src/lang/is-valid-date.js",
      "/src/polling/augment-entry-content.js",
      "/src/polling/deserialize-feed.js",
      "/src/polling/fetch-feed.js",
      "/src/polling/poll-feeds.js",
      "/src/storage/entry-store.js",
      "/src/storage/clear-entry-store.js",
      "/src/storage/count-unread-entries.js",
      "/src/storage/feed-store.js",
      "/src/storage/find-entry-by-link.js",
      "/src/storage/open-indexeddb.js",
      "/src/storage/store-entry.js",
      "/src/storage/upgrade-indexeddb.js",
      "/src/url/get-schemeless-url.js",
      "/src/url/resolve-url.js",
      "/src/url/rewrite-url.js",

      "/src/extension/on-chrome-alarm.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "background",
    "idle",
    "notifications"
  ],
  "content_security_policy":
    "script-src 'self'; object-src 'self' https://vimeo.com https://www.youtube.com https://www.ustream.tv; frame-src http: https:"
}
