{
  "version": "110",
  "manifest_version": 2,
  "minimum_chrome_version" : "47",
  "name": "Lucubrate",
  "author": "Joshua Froelich",
  "homepage_url": "https://github.com/jfroelich/rss-reader",
  "description": "Reading by artificial light late into the night",
  "browser_action": {
    "default_icon": "/images/rss_icon_trans.png",
    "default_title": "Lucubrate"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/lib/parse-srcset.js",
      "/src/add-entry.js",
      "/src/add-feed.js",
      "/src/archive-entries.js",
      "/src/background-page.js",
      "/src/entry.js",
      "/src/favicon-cache.js",
      "/src/favicon-service.js",
      "/src/feed.js",
      "/src/fetch-feed.js",
      "/src/fetch-html-service.js",
      "/src/indexeddb.js",
      "/src/parse-feed.js",
      "/src/polling-service.js",
      "/src/polling-service-test.js",
      "/src/string-utils.js",
      "/src/update-badge-unread-count.js",
      "/src/update-feed.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "http://*/*",
    "https://*/*",
    "idle",
    "notifications",
    "tabs",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "background",
    "idle",
    "notifications"
  ],
  "content_security_policy":
    "script-src 'self'; object-src 'self' https://vimeo.com https://www.youtube.com https://www.ustream.tv; frame-src http: https:"
}
