{
  "version": "110",
  "manifest_version": 2,
  "minimum_chrome_version" : "47",
  "name": "Lucubrate",
  "author": "Joshua Froelich",
  "homepage_url": "https://github.com/jfroelich/rss-reader",
  "description": "Reading by artificial light late into the night",
  "browser_action": {
    "default_icon": "/images/rss_icon_trans.png",
    "default_title": "Lucubrate"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/lib/parse-srcset.js",
      "/src/alarms.js",
      "/src/archive-entries.js",
      "/src/db.js",
      "/src/feed-parser.js",
      "/src/fetch-feed.js",
      "/src/on-badge-click.js",
      "/src/on-installed.js",
      "/src/poll.js",
      "/src/replace-html.js",
      "/src/resolve-urls.js",
      "/src/rewrite-url.js",
      "/src/truncate-html-string.js",
      "/src/update-badge-unread-count.js",
      "/src/utils.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "http://*/*",
    "https://*/*",
    "idle",
    "notifications",
    "tabs",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "background",
    "idle",
    "notifications"
  ],
  "content_security_policy":
    "script-src 'self'; object-src 'self' https://vimeo.com https://www.youtube.com https://www.ustream.tv; frame-src http: https:"
}
